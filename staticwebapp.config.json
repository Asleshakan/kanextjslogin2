{
  "routes": [
    {
      "route": "/api/auth/logout",
      "redirect": "/",
      "statusCode": 302
    },
    {
      "route": "/api/auth/*",
      "headers": {
        "cache-control": "no-store, must-revalidate"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "no-store, must-revalidate"
      }
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate"
  }
}
